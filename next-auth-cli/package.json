{
  "name": "next-auth-cli",
  "description": "next-auth command line utility",
  "version": "0.1.0-1",
  "type": "module",
  "module": "index.js",
  "types": "index.d.ts",
  "bin": {
    "next-auth-cli": "cli/bin.js"
  },
  "author": "Mr D.",
  "license": "MIT",
  "dependencies": {
    "esm": "^3.2.25",
    "next-auth": "3.1.0"
  },
  "peerDependencies": {
    "mongodb": "^3.5.9",
    "mssql": "^6.2.1",
    "mysql": "^2.18.1",
    "pg": "^8.3.0",
    "sqlite3": "^5.0.0"
  },
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/mocha": "^8.0.0",    
    "mocha": "^8.0.1",
    "mongodb": "^3.5.9",
    "mssql": "^6.2.1",
    "mysql": "^2.18.1",
    "pg": "^8.3.0",
    "prettier": "^2.0.5",
    "sqlite3": "^5.0.0",
    "typescript": "^3.9.7"
  },
  "peerDependenciesMeta": {
    "mongodb": {
      "optional": true
    },
    "mssql": {
      "optional": true
    },
    "mysql": {
      "optional": true
    },
    "pg": {
      "optional": true
    },
    "sqlite3": {
      "optional": true
    }
  },
  "files": [
    "cli",
    "types",
    "*.js",
    "*.d.ts"
  ],
  "scripts": {
    "pretty": "prettier",
    "prebuild": "rm types -rf",
    "build": "tsc -p .",
    "test:local": "yarn docker:down && yarn docker:up && sleep 10 && yarn test",
    "test": "mocha --exit",
    "docker:help": "echo usage:\n\t\"yarn docker <up|down> [-d]\"\n\t will launch \"docker-compose ../docker/docker-compose [...args]\"",
    "docker": "docker-compose -f ../docker/docker-compose.yml",
    "docker:up": "docker-compose -f ../docker/docker-compose.yml up -d",
    "docker:down": "docker-compose -f ../docker/docker-compose.yml down",
    "start": "node cli/bin.js",
    "debug": "node --inspect cli/bin.js",
    "debug:brk": "node --inspect-brk cli/bin.js ",
    "precommit": "yarn pretty && yarn build && yarn test"
  }
}
